{
  "kind": "collectionType",
  "collectionName": "operator_brands",
  "info": {
    "singularName": "operator-brand",
    "pluralName": "operator-brands",
    "displayName": "Brand"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "logo": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files"
      ]
    },
    "headquarters": {
      "type": "string"
    },
    "year_founded": {
      "type": "string"
    },
    "parent_company": {
      "type": "string"
    },
    "is_licensed": {
      "type": "boolean"
    },
    "description": {
      "type": "customField",
      "customField": "plugin::ckeditor5.CKEditor",
      "options": {
        "preset": "defaultHtml"
      }
    },
    "operator_countries": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::operator-country.operator-country",
      "mappedBy": "brand"
    }
  }
}
