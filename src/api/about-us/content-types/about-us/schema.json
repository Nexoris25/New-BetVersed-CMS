{
  "kind": "singleType",
  "collectionName": "about_uses",
  "info": {
    "singularName": "about-us",
    "pluralName": "about-uses",
    "displayName": "About Us"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "page_title": {
      "type": "string"
    },
    "short_title": {
      "type": "string"
    },
    "featured_image": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files"
      ]
    },
    "seoMeta": {
      "type": "component",
      "component": "shared.seo",
      "repeatable": false
    },
    "main_content": {
      "type": "customField",
      "customField": "plugin::ckeditor5.CKEditor",
      "options": {
        "preset": "defaultHtml"
      }
    },
    "slug": {
      "type": "uid",
      "targetField": "short_title"
    }
  }
}
