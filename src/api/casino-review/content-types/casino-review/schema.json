{
  "kind": "collectionType",
  "collectionName": "casino_reviews",
  "info": {
    "singularName": "casino-review",
    "pluralName": "casino-reviews",
    "displayName": "Casino Review"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "pageTitle": {
      "type": "string"
    },
    "short_title": {
      "type": "string"
    },
    "leadParagraph": {
      "type": "customField",
      "customField": "plugin::ckeditor5.CKEditor",
      "options": {
        "preset": "defaultHtml"
      }
    },
    "seoMeta": {
      "type": "component",
      "component": "shared.seo",
      "repeatable": false
    },
    "isFeatured": {
      "type": "boolean"
    },
    "isRecommended": {
      "type": "boolean"
    },
    "review_summary": {
      "type": "component",
      "component": "content.review-summary",
      "repeatable": false
    },
    "main_content": {
      "type": "dynamiczone",
      "components": [
        "content.rich-text",
        "shared.cta-button",
        "unit.label-and-value",
        "unit.responsible-gambling",
        "content.highligted-bonus-offer",
        "content.heading"
      ]
    },
    "faqs": {
      "type": "component",
      "component": "shared.fa-qs-section",
      "repeatable": false
    },
    "source_refernces": {
      "type": "component",
      "component": "content.source-reference",
      "repeatable": true
    },
    "last_tested_date": {
      "type": "date"
    },
    "casino_bonus": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::casino-bonus.casino-bonus",
      "mappedBy": "related_casino_review"
    },
    "highlighted_bonus_offer": {
      "type": "component",
      "component": "content.highligted-bonus-offer",
      "repeatable": false
    },
    "sportsbook_bonus": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::sportsbook-bonus.sportsbook-bonus",
      "inversedBy": "related_casino_review"
    },
    "sportsbook_review": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::sportsbook-review.sportsbook-review",
      "inversedBy": "related_casino_review"
    },
    "casino_games": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::casino-game.casino-game",
      "mappedBy": "casino_reviews"
    },
    "software_providers": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::software-provider.software-provider",
      "inversedBy": "casino_reviews"
    },
    "written_by": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::author.author",
      "inversedBy": "casino_reviews"
    },
    "casino_features": {
      "type": "component",
      "component": "casino.casino-features",
      "repeatable": false
    },
    "operator_country": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::operator-country.operator-country",
      "inversedBy": "casino_review"
    },
    "alternatives": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::operator-country.operator-country",
      "inversedBy": "casino_reviews"
    },
    "slug": {
      "type": "uid",
      "targetField": "short_title"
    },
    "fact_checked_by": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::author.author",
      "inversedBy": "casino_review"
    },
    "tldr_summary": {
      "type": "component",
      "component": "shared.tldr",
      "repeatable": false
    }
  }
}
