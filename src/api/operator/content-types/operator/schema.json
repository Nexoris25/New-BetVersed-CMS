{
  "kind": "collectionType",
  "collectionName": "operators",
  "info": {
    "singularName": "operator",
    "pluralName": "operators",
    "displayName": "Operator"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "operatorName": {
      "type": "string"
    },
    "logo": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files"
      ]
    },
    "headquarters": {
      "type": "string"
    },
    "yearFounded": {
      "type": "date"
    },
    "parentCompany": {
      "type": "string"
    },
    "official_website": {
      "type": "string"
    },
    "description": {
      "type": "customField",
      "customField": "plugin::ckeditor5.CKEditor",
      "options": {
        "preset": "defaultHtml"
      }
    },
    "globalLicenseInfo": {
      "type": "customField",
      "customField": "plugin::ckeditor5.CKEditor",
      "options": {
        "preset": "defaultHtml"
      }
    },
    "casino_reviews": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::casino-review.casino-review",
      "mappedBy": "casino_provider"
    }
  }
}
