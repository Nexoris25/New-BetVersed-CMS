{
  "kind": "collectionType",
  "collectionName": "operator_countries",
  "info": {
    "singularName": "operator-country",
    "pluralName": "operator-countries",
    "displayName": "Operator Country"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "country": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::country.country",
      "inversedBy": "operator_countries"
    },
    "operating_company": {
      "type": "string"
    },
    "license_detail": {
      "type": "component",
      "component": "shared.license-detail",
      "repeatable": true
    },
    "local_support": {
      "type": "component",
      "component": "shared.contact-detail",
      "repeatable": false
    },
    "operator_brand": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::operator-brand.operator-brand",
      "inversedBy": "operator_countries"
    },
    "sportsbook_reviews": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::sportsbook-review.sportsbook-review",
      "mappedBy": "operator_country"
    },
    "sportsbook_bonuses": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::sportsbook-bonus.sportsbook-bonus",
      "mappedBy": "operator_country"
    },
    "casino_bonuses": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::casino-bonus.casino-bonus",
      "mappedBy": "operator_country"
    },
    "casino_reviews": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::casino-review.casino-review",
      "mappedBy": "operator_country"
    },
    "operator_local_name": {
      "type": "string"
    },
    "notes": {
      "type": "customField",
      "customField": "plugin::ckeditor5.CKEditor",
      "options": {
        "preset": "defaultHtml"
      }
    },
    "operator_country_features": {
      "type": "component",
      "component": "operator-country.operator-country-features",
      "repeatable": false
    },
    "entry_year": {
      "type": "string"
    },
    "operator_country_cta": {
      "type": "component",
      "component": "shared.cta-button",
      "repeatable": true
    },
    "payment_methods": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::payment-method.payment-method",
      "inversedBy": "operator_countries"
    }
  }
}
