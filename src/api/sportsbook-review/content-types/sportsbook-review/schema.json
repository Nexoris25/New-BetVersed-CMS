{
  "kind": "collectionType",
  "collectionName": "sportsbook_reviews",
  "info": {
    "singularName": "sportsbook-review",
    "pluralName": "sportsbook-reviews",
    "displayName": "Sportsbook Review"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "pageTitle": {
      "type": "string"
    },
    "shortTitle": {
      "type": "string"
    },
    "public_url": {
      "type": "component",
      "component": "shared.public-url",
      "repeatable": false
    },
    "operator_brand": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::operator-brand.operator-brand",
      "inversedBy": "sportsbook_reviews"
    },
    "leadParagraph": {
      "type": "customField",
      "customField": "plugin::ckeditor5.CKEditor",
      "options": {
        "preset": "defaultHtml"
      }
    },
    "seoMeta": {
      "type": "component",
      "component": "shared.seo",
      "repeatable": false
    },
    "isFeatured": {
      "type": "boolean"
    },
    "isRecommended": {
      "type": "boolean"
    },
    "review_summary": {
      "type": "component",
      "component": "content.review-summary",
      "repeatable": false
    },
    "prosCons_title": {
      "type": "string"
    },
    "prosCons": {
      "type": "component",
      "component": "shared.pros-cons",
      "repeatable": false
    },
    "mainContent": {
      "type": "dynamiczone",
      "components": [
        "content.rich-text",
        "shared.pros-cons"
      ]
    },
    "faqs": {
      "type": "component",
      "component": "shared.fa-qs-section",
      "repeatable": false
    },
    "source_refernces": {
      "type": "component",
      "component": "content.source-reference",
      "repeatable": true
    },
    "last_tested_date": {
      "type": "date"
    },
    "highlighted_bonus_offer": {
      "type": "component",
      "component": "content.highligted-bonus-offer",
      "repeatable": false
    },
    "review_country": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::country.country",
      "inversedBy": "sportsbook_reviews"
    },
    "related_casino_review": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::casino-review.casino-review",
      "mappedBy": "sportsbook_review"
    },
    "related_sportsbook_bonus": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::sportsbook-bonus.sportsbook-bonus",
      "mappedBy": "sportsbook_review"
    },
    "related_casino_bonus": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::casino-bonus.casino-bonus",
      "mappedBy": "sportsbook_review"
    },
    "fact_checked_by": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::author.author",
      "inversedBy": "sportsbook_reviews"
    },
    "operator_country": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::operator-country.operator-country",
      "inversedBy": "sportsbook_reviews"
    },
    "sportsbook_features": {
      "type": "component",
      "component": "sportsbook.sportsbook-features",
      "repeatable": false
    }
  }
}
