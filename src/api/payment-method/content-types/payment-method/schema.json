{
  "kind": "collectionType",
  "collectionName": "payment_methods",
  "info": {
    "singularName": "payment-method",
    "pluralName": "payment-methods",
    "displayName": "Payment Method"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "transaction_time": {
      "type": "string"
    },
    "fees": {
      "type": "string"
    },
    "min_deposit": {
      "type": "string"
    },
    "max_payout": {
      "type": "string"
    },
    "crypto_payments_supported": {
      "type": "boolean"
    },
    "operator_countries": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::operator-country.operator-country",
      "mappedBy": "payment_methods"
    },
    "payment_channels": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::payment-channel.payment-channel",
      "mappedBy": "payment_methods"
    },
    "shortSummary": {
      "type": "customField",
      "customField": "plugin::ckeditor5.CKEditor",
      "options": {
        "preset": "defaultHtml"
      }
    }
  }
}
