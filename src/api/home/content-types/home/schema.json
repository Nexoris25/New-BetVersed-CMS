{
  "kind": "singleType",
  "collectionName": "homes",
  "info": {
    "singularName": "home",
    "pluralName": "homes",
    "displayName": "Home"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "page_title": {
      "type": "string"
    },
    "hero_section": {
      "type": "component",
      "component": "content.hero-section",
      "repeatable": false
    },
    "seoMeta": {
      "type": "component",
      "component": "shared.seo",
      "repeatable": false
    },
    "richText": {
      "type": "customField",
      "customField": "plugin::ckeditor5.CKEditor",
      "options": {
        "preset": "defaultHtml"
      }
    },
    "faqs": {
      "type": "component",
      "component": "shared.fa-qs-section",
      "repeatable": false
    },
    "support_organizations": {
      "type": "component",
      "component": "unit.responsible-gambling",
      "repeatable": true
    },
    "featured_countries": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::country.country"
    },
    "featured_brands": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::operator-brand.operator-brand"
    },
    "featured_articles": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::article.article"
    },
    "homepage_categories": {
      "type": "component",
      "component": "content.homepage-category-block",
      "repeatable": true
    }
  }
}
