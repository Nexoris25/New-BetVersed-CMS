{
  "kind": "collectionType",
  "collectionName": "casino_games",
  "info": {
    "singularName": "casino-game",
    "pluralName": "casino-games",
    "displayName": "Casino Game"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    },
    "game_type": {
      "type": "enumeration",
      "enum": [
        "Slots",
        "Sackpot Slots",
        "Table Games",
        "Roulette",
        "Blackjack",
        "Baccarat",
        "Poker",
        "Live Casino",
        "Crash Games",
        "Instants",
        "Keno",
        "Lottery",
        "Bingo"
      ]
    },
    "rtp": {
      "type": "decimal"
    },
    "volatility": {
      "type": "enumeration",
      "enum": [
        "Low",
        "Medium",
        "High"
      ]
    },
    "software_provider": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::software-provider.software-provider",
      "inversedBy": "casino_games"
    },
    "casino_reviews": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::casino-review.casino-review",
      "inversedBy": "casino_games"
    }
  }
}
