{
  "kind": "collectionType",
  "collectionName": "casino_games",
  "info": {
    "singularName": "casino-game",
    "pluralName": "casino-games",
    "displayName": "Casino Game"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    },
    "game_type": {
      "type": "enumeration",
      "enum": [
        "Slots",
        "Live Dealer",
        "Table Games",
        "Video Poker",
        "Crash & Instant Games",
        "Fishing & Arcade",
        "Virtual Sports"
      ]
    },
    "volatility": {
      "type": "enumeration",
      "enum": [
        "Low",
        "Low–Medium",
        "Medium",
        "Medium–High",
        "Medium–Very High",
        "High",
        "High–Very High",
        "Very High",
        "Adjustable",
        "Player-dependent"
      ]
    },
    "software_providers": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::software-provider.software-provider",
      "inversedBy": "casino_games"
    },
    "casino_reviews": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::casino-review.casino-review",
      "inversedBy": "casino_games"
    },
    "rtp": {
      "type": "string"
    }
  }
}
