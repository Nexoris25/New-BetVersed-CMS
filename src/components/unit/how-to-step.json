{
  "collectionName": "components_unit_how_to_steps",
  "info": {
    "displayName": "howToStep"
  },
  "options": {},
  "attributes": {
    "stepTitle": {
      "type": "string",
      "maxLength": 120
    },
    "stepDescription": {
      "type": "customField",
      "customField": "plugin::ckeditor5.CKEditor",
      "options": {
        "preset": "defaultHtml"
      }
    },
    "stepImage": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files"
      ]
    },
    "stepTime": {
      "type": "string"
    }
  },
  "config": {}
}
