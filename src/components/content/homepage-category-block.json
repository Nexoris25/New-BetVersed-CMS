{
  "collectionName": "components_content_homepage_category_blocks",
  "info": {
    "displayName": "Homepage Category Block"
  },
  "options": {},
  "attributes": {
    "category_title": {
      "type": "string"
    },
    "short_description": {
      "type": "customField",
      "customField": "plugin::ckeditor5.CKEditor",
      "options": {
        "preset": "defaultHtml"
      }
    },
    "category": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::category.category"
    },
    "articles": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::article.article"
    },
    "ctaButton": {
      "type": "component",
      "component": "shared.cta-button",
      "repeatable": false
    }
  },
  "config": {}
}
